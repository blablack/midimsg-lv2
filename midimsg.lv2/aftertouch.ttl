@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix atom:	<http://lv2plug.in/ns/ext/atom#> .

<http://github.com/blablack/midimsg-lv2/aftertouch>
	a lv2:Plugin, lv2:UtilityPlugin ;
	lv2:project <http://github.com/blablack/midimsg-lv2/> ;
	doap:name "Midi AfterTouch (Channel Pressure)" ;
	doap:license <http://opensource.org/licenses/isc> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort , atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ,
		<http://lv2plug.in/ns/ext/patch#Message> ;
		lv2:index 0;
		lv2:symbol "input" ;
		lv2:name "Input" ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "logarithmic" ;
		lv2:name "Logarithmic" ;
		lv2:portProperty lv2:toggled ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
       	lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "minimum" ;
		lv2:name "Minimum" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
       	lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "maximum" ;
		lv2:name "Maximum" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
       	lv2:maximum 1.0 ;
	] , [
		a lv2:OutputPort, lv2:CVPort ;
		lv2:index 4 ;
		lv2:symbol "output_cv" ;
		lv2:name "Output - CV" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
       	lv2:maximum 1.0 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "output_control" ;
		lv2:name "Output - Control" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
       	lv2:maximum 1.0 ;
	].
